{
  "name": "dms-conversion",
  "version": "2.0.1",
  "description": "A JavaScript library for converting between decimal degrees and degrees, minutes, and seconds (DMS).",
  "main": "dms.js",
  "types": "dms.d.ts",
  "jsnext:main": "es2015/dms.js",
  "files": [
    "dms.js",
    "dms.d.ts",
    "es2015/dms.js"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jasmine",
    "build:tsc": "tsc && node scripts/movedts",
    "build:babel": "babel es2015/dms.js > dms.js",
    "build:docs": "jsdoc2md es2015/dms.js --template README.hbs > README.md",
    "build": "npm run build:tsc && npm run build:babel && npm run build:docs",
    "prepublish": "npm run-script build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/dms-js.git"
  },
  "keywords": [
    "DMS",
    "decimal",
    "degrees",
    "degree",
    "minute",
    "second",
    "GIS"
  ],
  "author": "WSDOT-GIS",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/dms-js/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/dms-js#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "fs-extra": "^0.30.0",
    "jasmine": "^2.4.1",
    "jsdoc-to-markdown": "^1.3.6",
    "typescript": "^1.8.10"
  }
}
